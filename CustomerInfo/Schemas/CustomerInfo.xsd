    		<element name="IdentificationNo" type="string" maxOccurs="1" minOccurs="1"></element>
    	</sequence>
    </complexType>
    
    <complexType name="custInfoResType">
    	<sequence>
    		<element name="CIF" type="string" maxOccurs="1"
    			minOccurs="1">
