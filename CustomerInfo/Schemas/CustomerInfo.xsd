<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://bidv.service.envelop/data/CustomerInfo" xmlns:tns="http://www.example.org/CustomerInfo" elementFormDefault="qualified" xmlns:Q1="http://bidv.service.envelop/data/CustomerInfo">

